{
  "expo": {
    "name": "SOPSC Mobile App",
    "slug": "sopsc-mobile-app",
    "scheme": "sopsc",
    "icon": "./assets/images/AppIcon.png",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "modular_headers": true,
            "extraPods": [
              {
                "name": "FirebaseAuth", 
                "modular_headers": true
              },
              {
                "name": "FirebaseCore",
                "modular_headers": true
              },
              {
                "name": "FirebaseFirestore",
                "modular_headers": true
              },
              {
                "name": "GoogleUtilities",
                "modular_headers": true
              },
              {
                "name": "FirebaseCoreExtension",
                "modular_headers": true
              },
              {
                "name": "FirebaseAppCheckInterop",
                "modular_headers": true
              },
              {
                "name": "RecaptchaInterop",
                "modular_headers": true
              },
              {
                "name": "FirebaseFirestoreInternal",
                "modular_headers": true
              }
            ]
          }
        }
      ],
      [
        "expo-secure-store"
      ],
      [
        
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.203699688611-5uibr1f84mjjdn3b80920r21p8vuohho"
        }
      ]
    ],
    "android": {
      "package": "com.sirmrtyler.sopscmobileapp",
      "icon": "./assets/images/notification_icon_96.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "sopsc.app"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "a905a038-d584-4e7e-a7f9-cd2091702dc1"
      },
      "APP_VARIANT": "production",
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "191358823571-lu4grtd5hqslue3h04m2hjof1jsb3j4d.apps.googleusercontent.com",
      "EXPO_PUBLIC_API_URL": "https://sopsc-api-e3c7fmfvcagyh0d0.westus-01.azurewebsites.net/api/"
    },
    "ios": {
      "bundleIdentifier": "com.sirmrtyler.sopscmobileapp",
      "icon": "./assets/images/notification_icon_96.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:sopsc.app"]
    }
  }
}
